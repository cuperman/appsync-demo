{
  "name": "appsync-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "deploy": "npm run stack:deploy && npm run stack:describe",
    "lint": "eslint .",
    "s3:sync": "aws s3 sync ./public s3://appsync.jeffws.com/ --acl public-read",
    "stack:deploy": "aws cloudformation deploy --template-file ./template.yaml --stack-name appsync --capabilities CAPABILITY_IAM",
    "stack:describe": "aws cloudformation describe-stacks --stack-name appsync | tee ./stacks.json",
    "stack:destroy": "aws cloudformation delete-stack --stack-name appsync",
    "start": "webpack-dev-server --progress --colors --content-base public/",
    "test": "npm run lint"
  },
  "author": "Jeff Cooper",
  "license": "MIT",
  "dependencies": {
    "aws-amplify": "^0.4.4",
    "aws-amplify-react": "^0.1.50",
    "lodash": "^4.17.10",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^6.1.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "redux-devtools-extension": "^2.13.2",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4"
  }
}
